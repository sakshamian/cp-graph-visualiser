{
  "name": "cp-graph-visualizer",
  "displayName": "CP Graph Visualizer",
  "description": "Visualize graphs for competitive programming",
  "icon": "media/logo.png",
  "version": "0.0.2",
  "publisher": "SakshamNegi",
  "engines": {
    "vscode": "^1.103.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sakshamian/cp-graph-visualiser"
  },
  "categories": [
    "Programming Languages",
    "Visualization"
  ],
  "activationEvents": [
    "onView:graphVisualizerView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "graphVisualizer",
          "title": "CP Graph Visualizer",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "graphVisualizer": [
        {
          "id": "graphVisualizer.view",
          "name": "",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "typescript": "^5.0.0"
  }
}